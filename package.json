  },
  "homepage": "https://github.com/nosaku/ng2-bundle#readme",
  "devDependencies": {
    "gulp": "^3.9.1",
    "npm-run-all": "^1.8.0",
    "systemjs-builder": "^0.15.16",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "copy": "^0.2.0"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
