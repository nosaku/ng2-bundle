    "libs-bundle": "./node_modules/.bin/gulp libs.bundle",
    "copy-lib-deps": "./node_modules/.bin/gulp copy.lib.deps",
    "copy-example": "./node_modules/.bin/gulp copy.example",
    "bundle": "npm-run-all -s libs-bundle copy-lib-deps copy-example"
  },
